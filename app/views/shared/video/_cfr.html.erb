<div class="home-supply-basis-map-video home-supply-basis-map-video_cfr">
  <div class="home-supply-basis-map-video__wrapper">
    <div class="home-supply-basis-map-video__poster"><img src="/assets/video-map-cfr.png" alt=""/></div>
    <video class="home-supply-basis-map-video__path home-supply-basis-map-video__path_cfr-01" data-popup="cfr-01" loop="true" muted="muted" playsinline="playsinline">
      <source src="/assets/video/cfr-01.mp4" type="video/mp4"/>
      <source src="/assets/video/cfr-01.webm" type="video/webm"/>
    </video>
    <video class="home-supply-basis-map-video__path home-supply-basis-map-video__path_cfr-02" data-popup="cfr-02" loop="true" muted="muted" playsinline="playsinline">
      <source src="/assets/video/cfr-02.mp4" type="video/mp4"/>
      <source src="/assets/video/cfr-02.webm" type="video/webm"/>
    </video>
    <div class="home-supply-basis-map-video__dot home-supply-basis-map-video__dot_cfr-01" data-video="cfr-01"></div>
    <div class="home-supply-basis-map-video__dot home-supply-basis-map-video__dot_cfr-02" data-video="cfr-02"></div>
    <div class="home-supply-basis-map-video__city home-supply-basis-map-video__city_cfr-01">Тунцзянь</div>
    <div class="home-supply-basis-map-video__city home-supply-basis-map-video__city_cfr-02">Фуюань</div>
    <div class="home-supply-basis-map-video__city home-supply-basis-map-video__city_cfr-03">Хабаровск</div>
    <div class="home-supply-basis-map-video-popup home-supply-basis-map-video-popup_cfr-01">
      <div class="home-supply-basis-map-video-popup__close"><img src="/assets/icons/close.svg" alt=""/></div>
      <div class="home-supply-basis-map-video-popup__head">
        <div class="home-supply-basis-map-video-popup__type">Речной порт</div>
        <div class="home-supply-basis-map-video-popup__city">Фуюань</div>
      </div>
      <div class="home-supply-basis-map-video-popup__type">Базис доставки</div>
      <div class="home-supply-basis-map-video-popup__title">cfr</div>
      <button class="button button_primary popup-open" data-popup="popup-order">
        <%= @buttons['order'][0]['title'] %>
      </button>
    </div>
    <div class="home-supply-basis-map-video-popup home-supply-basis-map-video-popup_cfr-02">
      <div class="home-supply-basis-map-video-popup__close"><img src="/assets/icons/close.svg" alt=""/></div>
      <div class="home-supply-basis-map-video-popup__head">
        <div class="home-supply-basis-map-video-popup__type">Речной порт</div>
        <div class="home-supply-basis-map-video-popup__city">Тунцзян</div>
      </div>
      <div class="home-supply-basis-map-video-popup__type">Базис доставки</div>
      <div class="home-supply-basis-map-video-popup__title">cfr</div>
      <button class="button button_primary popup-open" data-popup="popup-order">
        <%= @buttons['order'][0]['title'] %>
      </button>
    </div>
  </div>
</div>
